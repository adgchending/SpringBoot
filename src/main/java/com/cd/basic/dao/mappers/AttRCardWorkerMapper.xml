    <select id="selectAttRCardWorker" parameterType="com.cd.common.Assist"  resultMap="result_AttRCardWorker_Map">
        select  <if test="distinct !=null">${distinct}</if>
        <choose>
            <when test="resultColumn!=null">${resultColumn}</when>
